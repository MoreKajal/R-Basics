# Find calculate roots of  : x^2 + x -1
a<- 1
b<- 1
c<- -1

(-b + sqrt(b^2 - 4*a*c) )/ (2 * a)

log(a)
exp(1)
# nested functions are evaluated inside out
log(exp(1))
# To get which arguments are taken by a  function
args(log)
log(8, base = 2)  # Note here we have used equal sign not assignment sign

pi
Inf

